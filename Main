                [1] = "StartQuest",
                [2] = "DesertQuest",
                [3] = 1
            }


            


            _G.AutoFarm = true
            if _G.AutoFarm == true then 
                if  level >= 0 and level <= 10 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(argsbuso))
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                    while _G.AutoFarm == true do wait()
                        spawn(function()
                            Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Enemies["Bandit [Lv. 5]"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                            while true do wait(70)
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                            end
                            end)
                    end  
                end
                if  level > 10 and level <= 15 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(argsbuso))
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args2))
                    while _G.AutoFarm == true do wait()
                        spawn(function()
                            Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Enemies["Monkey [Lv. 14]"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                            while true do wait(70)
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args2))
                            end
                        end)
                            
                    end    
                end
                
                if  level > 15 and level <= 30 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args3))
                    while _G.AutoFarm == true do wait()
                        spawn(function()
                            Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Enemies["Gorilla [Lv. 20]"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                            while true do wait(70)
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args3))
                            end
                        end)
                            
                    end    
                end
                
                if  level > 30 and level <= 40 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args4))
                    while _G.AutoFarm == true do wait()
                        spawn(function()
                            Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Enemies["Pirate [Lv. 35]"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                            while true do wait(70)
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args4))
                            end
