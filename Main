    StatSection:NewToggle("Auto Melee", "Auto levels up melee", function(state)
        while state do
            _G.statadding = true
            while _G.statadding == true do
                
                local meleestat = Player.Data.Points.Value
                while meleestat > 0 do
                local args = {
                    [1] = "AddPoint",
                    [2] = "Melee",
                    [3] = 1
                }
                
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                if meleestat == 0 then
                    break
                end
                end
            end
        end
    end)
 
    StatSection:NewToggle("Auto Sword", "Auto levels up sword", function(state)
        if state then
            _G.statadding = true
            while _G.statadding == true do
                
                local meleestat = Player.Data.Points.Value
                while meleestat > 0 do
                local args = {
                    [1] = "AddPoint",
