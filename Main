                            end
                        end)
                            
                    end    
                end
                
                if  level > 60 and level <= 75 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args6))
                    while _G.AutoFarm == true do wait()
                        spawn(function()
                            Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Enemies["Desert Bandit [Lv. 60]"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
