    MainSection:NewToggle("Auto farm level", ":)", function(state)
        local level = Player.Data.Level.Value
        print (level)
        while state do 
            
            local argsbuso = {
                [1] = "Buso"
            }
            
            local args = {
                [1] = "StartQuest",
                [2] = "BanditQuest1",
                [3] = 1
            }
    

            local args2 = {
                [1] = "StartQuest",
                [2] = "JungleQuest",
                [3] = 1
            }
            
